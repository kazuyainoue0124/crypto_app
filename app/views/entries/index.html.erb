<% provide(:title, "ホーム") %>

<% @entries.each do |entry| %>
<div class="panel panel-default">
  <div class="panel-body">
    <%= link_to entry.title, entry %> - <i> published <%= time_ago_in_words(entry.published) %> ago.</i> - <i> <%= link_to entry.feed.name, entry.feed.url %> </i>
  </div>
</div>
<% end %>